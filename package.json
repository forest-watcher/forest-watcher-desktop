  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "deploy": "npm run build&&gh-pages -d build",
    "test": "react-scripts test --env=jsdom",
    "lint-staged": "lint-staged",
    "lint": "eslint --fix --ext .js,.jsx,.ts,.tsx ./src --fix",
    "precommit": "yarn run lint-staged",
    "transifex:push": "tx push",
    "transifex:pull": "tx pull -f && yarn transifex:prettier",
    "generate-interface:teams": "npx openapi-typescript https://raw.githubusercontent.com/wri/fw_teams/dev/docs/fw_teams.yaml --output ./src/interfaces/teams.ts",
    "generate-interface:api": "npx openapi-typescript https://raw.githubusercontent.com/wri/fw_api/dev/docs/fw_api.yaml --output ./src/interfaces/api.ts",
    "generate-interface:forms": "npx openapi-typescript https://raw.githubusercontent.com/wri/fw_forms/dev/docs/fw_forms.yaml --output ./src/interfaces/forms.ts",
    "generate-interface:exports": "npx openapi-typescript https://raw.githubusercontent.com/wri/fw_exports/dev/docs/fw_exports.yaml --output ./src/interfaces/exports.ts",
    "storybook": "start-storybook -p 6006 -s src/assets",
    "build-storybook": "build-storybook -s src/assets",
    "transifex:prettier": "prettier --ignore-path .gitignore \"src/locales/*.+(json)\" --write",
    "generate:api": "npx openapi-codegen gen api",
    "generate:teams": "npx openapi-codegen gen teams",
    "generate:exports": "npx openapi-codegen gen exports",
    "generate:forms": "npx openapi-codegen gen forms",
    "generate:core": "npx openapi-codegen gen core",
    "generate:new": "npx openapi-codegen init"
  },
  "prettier": "@3sidedcube/prettier-config",
  "eslintConfig": {
    "extends": [
      "react-app",
      "plugin:jsx-a11y/recommended",
      "plugin:prettier/recommended"
    ],
    "plugins": [
      "jsx-a11y"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "homepage": ".",
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": "eslint --fix"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "not ie 11",
      "not chrome < 51",
      "not safari < 10",
      "not android < 51"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
